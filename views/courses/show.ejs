<% include ../partials/header %>

<div class="container">
    <h1>My Courses</h1>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-8 well">
            <% currentUser.userProgress.forEach(function (el){ %>
                <div>
                    <h2><%= el.list.name %></h2>
                    <p><%= el.list.description %></p>
                </div>
                <hr>
            <% }); %>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>

<% include ../partials/footer %>